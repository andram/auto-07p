.SUFFIXES:
.SUFFIXES: .c++ .o

OBJS = gReadBif.o gReadSol.o drawCoords.o \
        normalizeSolData.o normalizeBifData.o stringtrim.o \
        tube.o polygon3d.o readFM.o createBoundingBox.o\
        createCoords.o axis.o createLegend.o gmain.o @SOXT_OBJS@ \
        rounding.o printToPS.o

CXX=@CXX@
INC=@top_srcdir@/plaut04/includes
INCLUDES=-I$(INC) @DEPS_INCLUDES@
CFLAGS=@CFLAGS@ @X_CFLAGS@
DEPS_LIBS=@DEPS_LIBS@
MOC=@MOC@
LIBS=$(DEPS_LIBS)

.c++.o:
	$(CXX) $(INCLUDES) $(CFLAGS) -c $< -o $@

@top_srcdir@/bin/plaut04: $(OBJS)
	$(CXX) $(CFLAGS) -o $@ $(OBJS) $(LIBS)

gmainqt.moc.c++: gmainqt.h
	$(MOC) $< -o $@

clean:
	rm -f *.o gmainqt.moc.c++

axis.o: $(INC)/gplaut04.h $(INC)/axis.h
createCoords.o: $(INC)/gplaut04.h $(INC)/axis.h
drawCoords.o: $(INC)/gplaut04.h
gmain.o: $(INC)/gplaut04.h $(INC)/gVarNames.h $(INC)/tube.h
gmainxt.o: $(INC)/gplaut04.h
gmainqt.o: $(INC)/gplaut04.h
gReadBif.o: $(INC)/gplaut04.h
gReadSol.o: $(INC)/gplaut04.h
normalizeBifData.o: $(INC)/gplaut04.h
normalizeSolData.o: $(INC)/gplaut04.h
polygon3d.o: $(INC)/polygon3d.h
readFM.o: $(INC)/gplaut04.h
tube.o: $(INC)/tube.h $(INC)/polygon3d.h
