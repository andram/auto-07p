.SUFFIXES:
.SUFFIXES: .c++ .o

OBJS = ../gReadBif.o ../gReadSol.o ../drawCoords.o toInertialFrame.o createDisk.o \
        createPrimary.o createLibPoint.o ../normalizeSolData.o ../normalizeBifData.o\
        ../stringtrim.o \
        printToPS.o ../tube.o ../polygon3d.o ../readFM.o ../createBoundingBox.o\
        ../createCoords.o ../axis.o ../createLegend.o gmain.o @SOXT_OBJS_R3B@ \
	../rounding.o gmainr3b.o

CXX=@CXX@
INC=@top_srcdir@/plaut04/includes
INCLUDES=-I$(INC) @DEPS_INCLUDES@
CFLAGS=@CFLAGS@ @X_CFLAGS@ -DR3B
DEPS_LIBS=@DEPS_LIBS@
LIBS=$(DEPS_LIBS)

.c++.o:
	$(CXX) $(INCLUDES) $(CFLAGS) -c $< -o $@

@top_srcdir@/bin/r3bplaut04: $(OBJS)
	$(CXX) $(CFLAGS) -o $@ $(OBJS) $(LIBS)

gmain.o: ../gmain.c++
	$(CXX) $(INCLUDES) $(CFLAGS) -c $< -o $@

gmainxt.o: ../gmainxt.c++
	$(CXX) $(INCLUDES) $(CFLAGS) -c $< -o $@

gmainqt.o: ../gmainqt.c++
	$(CXX) $(INCLUDES) $(CFLAGS) -c $< -o $@

clean:
	rm -f *.o

gmain.o: $(INC)/gplaut04.h $(INC)/gVarNames.h $(INC)/tube.h Makefile
gmainxt.o: $(INC)/gplaut04.h
gmainqt.o: $(INC)/gplaut04.h
gmainr3b.o: $(INC)/gplaut04.h
printToPS.o: Makefile
toInertialFrame.o: $(INC)/gplaut04.h
