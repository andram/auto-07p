#! /bin/sh
if [ -z "$AUTO_DIR" ]; then
    bindir=`dirname $0`
    AUTO_DIR=`dirname bindir`
    export AUTO_DIR
fi
PPATH=$AUTO_DIR/python
if [ -z "$PYTHONPATH" ]; then
    PYTHONPATH=$PPATH
else
    PYTHONPATH=$PPATH:$PYTHONPATH
fi
export PYTHONPATH

if test "`uname`" = "Darwin"; then
  if which pythonw > /dev/null; then
    exec pythonw ${1+"$@"};
  fi
fi
python ${1+"$@"};
