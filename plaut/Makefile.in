.SUFFIXES: .f .o
#
FC     = @FC@
FFLAGS = @OPT_FLAG@ -c 
SRC    = plautdev.f plautint.f plautlib.f plautmain.f
PGM    = ../bin/plaut
RM     = rm -f
#
.f.o:
	$(FC) $(FFLAGS) $*.f -o $@
#
$(PGM): $(SRC:.f=.o)
	$(FC) $(SRC:.f=.o) -o $@ 
#
clean:  
	$(RM) *.o

