#! /usr/bin/env autox

import sys
import parseB

# This is the Python syntax for making a script runable    
if __name__ == '__main__':
    if len(sys.argv) > 1:
        diagram=parseB.parseB('b.'+sys.argv[1],screen_lines=1)
    else:
        diagram=parseB.parseB('fort.7',screen_lines=1)
    diagram.writeScreen()

