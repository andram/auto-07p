#!/bin/sh
if ( test $# -eq 1 ) then
	file=s.$1
else
	file=fort.8
fi
if python - 2> /dev/null <<EOF
import sys
import os
sys.path.append(os.path.join(os.environ["AUTO_DIR"],"python"))
import parseS

solutions=parseS.parseS("$file")
print "\n  BR    PT  TY LAB"
for sol in solutions:
    typename = sol["Type name"]
    if typename == 'RG':
        typename = ''
    print "%4s%6s%4s%4s"%(sol["Branch number"], sol["Point number"],
        typename,sol["Label"])
EOF
then
    :
else
    $AUTO_DIR/bin/listlabels < $file
fi
